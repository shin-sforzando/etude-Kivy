#:kivy 1.10.1

<CustomPopup>:
    size_hint: .8, .8
    auto_dismiss: False
    title: 'Custom Popup'
    BoxLayout:
        orientation: 'horizontal'
        Button:
            text: 'A'
            on_press: root.on_press_a()
        Button:
            text: 'B'
            on_press: root.on_press_b()
